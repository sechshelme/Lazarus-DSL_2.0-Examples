      </Item>
      <Item>
        <Filename Value="pas_units/sdl3_pixels.pas"/>
        <UnitName Value="SDL3_pixels"/>
      </Item>
      <Item>
        <Filename Value="pas_units/sdl3_properties.pas"/>
